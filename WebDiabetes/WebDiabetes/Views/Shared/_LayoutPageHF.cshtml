                <nav class="navbar navbar-expand-lg bg-white navbar-light py-3 py-lg-0">
                    <a href="~/Home/Index" class="navbar-brand" id="logo_home">
                        <h1 class="m-0 text-uppercase color-primary"><i class="fas fa-hand-holding-medical me-2"></i>@*<i class="fa fa-clinic-medical me-2"></i>*@Diabetes</h1>
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarCollapse">
                        <div class="navbar-nav ms-auto py-0">
                            <a href="~/Home/Index" class="nav-item nav-link">Home</a>
                            <a href="~/Home/About" class="nav-item nav-link">About</a>
                            <div class="nav-item dropdown">
                                <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Diabetes</a>
                                <div class="dropdown-menu m-0">
                                    <a href="~/Home/Dataset" class="dropdown-item">Dataset</a>
                                    <a href="~/Home/Test" class="dropdown-item">Test</a>
                                </div>
                            </div>
                            <a href="~/Home/Contact" class="nav-item nav-link">Contact</a>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
        <!-- Navbar End -->
    </div>

    <div class="container mt-3">
        @RenderBody()
